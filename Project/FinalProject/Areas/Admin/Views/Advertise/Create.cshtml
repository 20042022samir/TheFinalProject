@model Advertise

<form method="post" style="margin:10px;" enctype="multipart/form-data">
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">description</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="description.." asp-for="@Model.Descripption">
        <span class="alert alert-text" asp-validation-for="@Model.Descripption" style="color:red; font-size:16px;"></span>
    </div>

    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">image</label>
        <input  class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="image.." asp-for="@Model.file">
        <span class="alert alert-text" asp-validation-for="@Model.file" style="color:red; font-size:16px;"></span>
    </div>

  

    <button type="submit">Submit</button>
</form>